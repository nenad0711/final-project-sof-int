pipeline {
    agent any



    stages {
        stage('Build') {
            steps {
                // Get some code from a GitHub repository
                git 'https://github.com/nenad0711/final-project-sof-int.git'
                bat "mvn clean compile"
            }
            }
            stage('Test'){
                 steps{
                 bat 'mvn test'
            }
                    }
            stage('Install'){
                             steps{
                             bat 'mvn clean install'
                        }
                              }

    }
}
